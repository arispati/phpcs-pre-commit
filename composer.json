{
    "name": "arispati/phpcs-pre-commit",
    "description": "Run phpcs before git commit",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arispati\\PhpcsPreCommit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Aris",
            "email": "masarispati@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arispati\\PhpcsPreCommit\\Providers\\PhpcsPreCommitServiceProvider"
            ]
        }
    }
}
